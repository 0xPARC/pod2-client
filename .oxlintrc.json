{
  "rules": {
    "no-restricted-globals": ["error", "event", "Buffer"],
    "no-commonjs": "error",
    "no-import-type-side-effects": "error",
    "typescript/no-non-null-asserted-nullish-coalescing": "error",
    "guard-for-in": "error",
    "no-unused-vars": ["warn", { "caughtErrors": "none", "args": "all", "vars": "all" } ]
  },
  "ignorePatterns": ["dist/**", "vite.config.js", "**/routeTree.gen.ts"]
}
